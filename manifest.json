{
  "short_name": "Reminder",
  "name": "Reminder",
  "description": "Remind yourself of anything from the share sheet",
  "start_url": "./",
  "share_target": {
    "action": "./?share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "icons": [{
    "src": "assets/icon-512.png",
    "sizes": "512x512",
    "type": "image/png"
  }],
  "background_color": "#202124",
  "display": "standalone",
  "theme_color": "#202124"
}
